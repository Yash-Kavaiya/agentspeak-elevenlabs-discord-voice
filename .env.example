# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# ElevenLabs Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id_here

# Optional: Voice ID for custom voice (leave empty for default)
ELEVENLABS_VOICE_ID=

# Bot Settings
# Auto-join voice channel when user joins (true/false)
AUTO_JOIN_VOICE=true

# Greeting message when user joins
GREETING_ENABLED=true
GREETING_MESSAGE=Hello! I'm your AI assistant. How can I help you today?

# Debug mode
DEBUG=false
